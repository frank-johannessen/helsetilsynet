<?xml version="1.0"?>

<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->

<episerver xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform" >
  <sites>
    <site>
      <siteSettings siteUrl="http://losen.helsetilsynet.no/" xdt:Transform="SetAttributes"/>
    </site>
  </sites>
  <virtualPath>
		<providers>			
      <add physicalPath="C:\EPiServer\VPP\Helsetilsynet.Intranet\PageFiles"
        name="SitePageFiles" xdt:Locator="Match(name)" xdt:Transform="SetAttributes" />
      <add physicalPath="C:\EPiServer\VPP\Helsetilsynet.Intranet\Global"
        name="SiteGlobalFiles" xdt:Locator="Match(name)" xdt:Transform="SetAttributes" />
      <add physicalPath="C:\EPiServer\VPP\Helsetilsynet.Intranet\Documents"
        name="SiteDocuments" xdt:Locator="Match(name)" xdt:Transform="SetAttributes" />
      <add physicalPath="C:\EPiServer\VPP\Helsetilsynet.Intranet\CommunityContributed\EPiServerCommunity\"
        name="EPiServerCommunity" xdt:Locator="Match(name)" xdt:Transform="SetAttributes" />
      <add physicalPath="C:\EPiServer\VPP\Helsetilsynet.Intranet\upload\"
        name="SiteUpload" xdt:Locator="Match(name)" xdt:Transform="SetAttributes" />
		</providers>
	</virtualPath>
</episerver>